{
  "name": "velocityrush3d",
  "version": "0.1.0",
  "description": "A full-featured 3D multiplayer racing game",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "build:prod": "vite build --mode production",
    "test": "node tests/testRunner.node.js",
    "test:physics": "node tests/testRunner.node.js --category physics",
    "test:performance": "node tests/testRunner.node.js --category performance",
    "test:stats": "node tests/testRunner.node.js --stats",
    "test:ci": "npm run lint && npm run test",
    "test:e2e": "node tests/e2eTests.js",
    "test:all": "npm run test && npm run test:e2e",
    "benchmark": "node tests/performanceBenchmark.js",
    "deploy": "node scripts/deploy.js",
    "deploy:netlify": "npm run deploy netlify",
    "deploy:vercel": "npm run deploy vercel",
    "build:mobile": "node scripts/build-mobile.js",
    "build:ios": "npm run build:mobile ios",
    "build:android": "npm run build:mobile android",
    "deploy:server": "node scripts/deploy-server.js",
    "server:logs": "docker-compose logs -f velocityrush-server",
    "server:stop": "docker-compose down",
    "analyze": "npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "cannon-es": "^0.20.0",
    "howler": "^2.2.3",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "three": "^0.154.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "express": "^4.18.2",
    "jsdom": "^22.1.0",
    "puppeteer": "^24.26.1",
    "terser": "^5.44.0",
    "vite": "^4.4.0"
  },
  "keywords": [
    "game",
    "3d",
    "racing",
    "three.js"
  ],
  "author": "",
  "license": "MIT"
}
